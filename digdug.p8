--movement


function move_left()
	
 movex-=p.speed
 p.spr+=1
	 if p.spr>2 then p.spr=1 end
 p.f=true
	p.dir="left"		
end
	 
--right
function move_right()
	
 movex+=p.speed
 p.spr+=1
 	if p.spr>2 then p.spr=1 end
 p.f=false
 p.dir="right"
end
--up
function move_up()
	
	movey-=p.speed
	if p.y<3 then p.spr=p.spr else
	p.spr=p.up end
 p.up+=1
	 if p.up>6 then p.up=5 end
	p.dir="up"
end	 
--down
function move_down()
	
 movey+=p.speed
 p.spr=p.down
 p.down+=0.5
 	if p.down>4 then p.down=3 end
	p.dir="down" 
	
end

	
