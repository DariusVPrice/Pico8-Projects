--map code

t=6 --ticker

function map_setups()
	wall=0
	fruit=1
	enemy=2
	dirt=3
	
end

function draw_map()
	map(0,0,0,0,128,128)
end

function is_tile(tile_type,x,y)
	local tile=mget(x,y)
	local has_flag=fget(tile,tile_type)
	return has_flag
end

function can_move(x,y)
	return not is_tile(wall,x,y)
end


function swap_tile(x,y)
	tile=mget(x,y)
	mset(x,y,tile+1)
end

function make_rock()
	r={}
	r.sprite=32
	r.x=6
	r.y=5
	r.g=r.y+0.09
	
end

function draw_rock()
	spr(r.sprite,r.x*8,r.y*8,1,1)
end

function update_rock()
		
	
end

