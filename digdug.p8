--states

function startgame()
	map_setups()
	make_player()
	make_pooka()
	make_rock()
	
end
 
function menu_show()
	
	cls()
	print("dig dug",40,40)
	print("press z to play",40,64)
	
end

function menu_upd()
	if btnp(4) then
	
		_upd=game_upd
		_drw=game_drw
		
		end
end


function menu_drw()

end


function game_upd()
	move_player()
		update_rock()
	_score()
	hit()
	pump()
	movepooka()
	anim_pooka()
	pump()
	
end
	
function game_drw()
	cls()
	draw_map()
	draw_player()
	draw_pooka()
		draw_rock()
	_score()
	hit()
	dig()
	pump()
	movepooka()
	anim_pooka()
	 
	
end

function upd_gameover()
	
	if btnp(4) then 
	
	_upd=game_upd
	_drw=game_drw end
	
	p.x=8	p.y=2
	 
end

function drw_gameover()
	cls()
	
	
	print("game over",40,64)
	print("press z to try again")
	
	

	
end
