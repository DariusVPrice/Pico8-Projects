--player code

function make_player()
 p={}
	p.x=8
	p.y=2
	p.speed=1
	p.spr=1
	p.win=0
	p.f=false
	p.down=3
	p.up=5
	p.lives=2
	p.dir=""
	--p.width=(p.x+1)
	--p.height=(p.y+1)
	end
	
function draw_player()
	spr(p.spr,p.x*8,p.y*8,1,1,p.f)
end
	
function move_player()
	
	movex=p.x
	movey=p.y
	
	if (btnp(⬅️)) then
		move_left()
	elseif (btnp(⬆️)) then
		move_up()
	elseif (btnp(➡️)) then
		move_right()
	elseif (btnp(⬇️)) then
		move_down()
	end
	 	
	if (can_move(movex,movey)) then
		p.x=mid(0,movex,15)
		p.y=mid(2,movey,14)
		end
	
end

function hit()
	if (p.x+1)>pk.x and
		(p.y+1)>pk.y and
		p.x<(pk.x+1) and
		p.y<(pk.y+1) then
	
			p.spr=25
		_upd=upd_gameover
		_drw=drw_gameover
	end

end

function dig()
 local check=fget(mget(p.x,p.y))
 if check==8 then		
	check=mget(x,y)
	mset(p.x,p.y,check+1)
	score+=100
end
		
 
end
 
function pump()
	local p=0
	_pump={13,14,15}

	if btnp(4) then
	p+=1
	spr(13,x,y)
	spr(14,x,y)
	spr(15,x,y)
	end	
end
