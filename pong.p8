-- game init

function main_menu()
	cls(5)
	
	end



function game_stt()

	p={}
	p.spr=1
	p.x=2.5
	p.y=3
	p.score=0
	
	b={} 
	b.mov=flr(rnd(10))
	b.spr=4
	b.x=8
	b.y=8
	
	c={}
	c.spr=3
	c.x=14
	c.y=b.y
	c.score=0

end
-->8
-- draw

function menu_drw()
	print("pong", 55,50)
	print("press x to start",35,60)
	
end

function game_drw()

	cls(5)
	map(0,0,8,8)
	spr(p.spr,p.x*8,p.y*8)
	spr(c.spr,c.x*8,c.y*8)
	spr(b.spr,b.x*8,b.y*8)
	
	print(p.score,5*8,1*8)
	print(c.score,11*8,1*8)
	
	
	ball_go()
	c.y=b.y
	scored()
	collide()	

end
-->8
--updates

function menu_upd()

	if btn(❎) then
		game_state=game_stt()
	 _drw=game_drw
		_upd=game_upd
	end

end

function game_upd()
	
	if btn(⬇️) then p.y+=1 end
	if btn(⬆️) then p.y-=1 end
	if p.y>=15 then p.y=15 end
	if p.y<=1 then p.y=1 end
	if c.y>=15 then c.y=15 end
	if c.y<=1 then c.y=1 end
	
	

end

