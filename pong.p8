--ball code	

function ball_go()
	 
	 print(b.mov)
		if b.mov % 2 == 0 then
		--b.y+=0.3 
		b.x-=0.3
		else
		--b.y+=0.3
	 b.x+=0.3
		end 
		
		
end	
	
function collide()
	
	if flr(b.x) == flr(c.x) then
		b.x -= 0.3 b.y += 0.3
		b.mov += 1
	end
	
	if flr(b.y) and flr(b.x) == flr(p.y) then
		b.x += 0.3 
		--b.y-= 0.3
		b.mov -=1
		print("good")
	end
		
end

function scored()

	if b.x>15 then
		p.score=p.score+1
		print ("good job",55,64,6)
	end
			
	if b.x<2 then
		c.score=c.score+1
		print ("you suck",55,64,6)
		
		
		end
		
end
